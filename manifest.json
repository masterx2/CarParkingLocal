{
  "name": "CarParking",
  "description": "CarParking Management",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "128": "parking-128.png" },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "storage",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ]
}